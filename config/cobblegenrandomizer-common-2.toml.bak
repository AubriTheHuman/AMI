
#General settings
[general]
	#Use config instead of datapack
	use_config = true

	#List settings
	#Syntax: ["modid:block|weight"]
	#Example: ["minecraft:stone|2","minecraft:dirt|1"]
	#Forge tags are supported
	[general.lists]
		#Cobble gen
		block_list_cobble = ["minecraft:cobblestone|60"]
		#Stone gen
		block_list_stone = ["minecraft:stone|30"]
		#Basalt gen
		block_list_basalt = ["minecraft:basalt|20", "minecraft:blackstone|20", "minecraft:netherrack|60"]

		#Custom settings
		[general.lists.custom]
			#Custom generators
			#Syntax: [gen]
			#Gen: [type, block, list]
			#Type: cobblestone, stone
			#Block: resource location of the block below the generated block
			#List: see List settings
			#Examples:
			#custom_generators = [
			#   ["cobblestone", "minecraft:diamond_block", ["minecraft:diamond_block"]],
			#   ["cobblestone", "minecraft:dirt", ["forge:dirt"]],
			#   ["cobblestone", "minecraft:white_wool", ["minecraft:wool"]]]
			custom_generators = [
				["stone", "amicore:quartzite_ore", ["minecraft:andesite"]],
				["stone", "amicore:nether_quartz_ore", ["minecraft:andesite"]],
				["stone", "byg:quartzite_sand", ["minecraft:diorite"]],
				["stone", "byg:raw_quartz_block", ["minecraft:granite"]],
				["cobblestone", "engineersdecor:dense_grit_sand_block", ["minecraft:sandstone"]],
				["stone", "betterendforge:flavolite", ["betterendforge:flavolite|60","minecraft:end_stone|40"]],
				["cobblestone", "amicore:sodalite", ["create:limestone|80","create:weathered_limestone|20"]],
				["stone", "amicore:regolith", ["create:gabbro"]],
				["cobblestone", "byg:scoria_stone", ["create:natural_scoria"]],
				["stone", "byg:soapstone", ["create:dolomite"]]]

